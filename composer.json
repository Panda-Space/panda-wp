{
    "name": "pandawp/generator",
    "type": "project",
    "description": "PandaWP CLI",
    "authors": [
        {
            "name": "Cleiver Valera",
            "email": "hola@cleivervalera.com"
        }
    ],
    "require": {
        "upstatement/routes": "*",
        "phpmailer/phpmailer": "6.1",
        "vlucas/phpdotenv": "^5.3",
        "illuminate/database": "^10.35",
        "timber/timber": "^2.0"
    },
    "require-dev": {
        "symfony/console": "^7.2",
        "kint-php/kint": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "PandaWP\\Console\\": "commands/",
            "Admin\\Pages\\": "app/functions/admin/pages/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
